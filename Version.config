TOOL=MacGPG2.1

version_parts=($(python3 -c <"$(dirname "${BASH_SOURCE[0]}")/libs.json" "import sys,json
print(next((item for item in json.loads(sys.stdin.read()) if item['name'] == 'gnupg'))['version'].replace('.', ' '))"))
if [[ ${#version_parts[@]} -ne 3 || "${version_parts[0]}" != "2" ]]; then
	echo "Unable to get MacGPG2 version!" >&2
	exit 1
fi


MAJOR=2
MINOR=${version_parts[1]}
REVISION=${version_parts[2]}
VERSION="${MAJOR}.${MINOR}${REVISION:+.$REVISION}${PRERELEASE:+-$PRELEASE}"
